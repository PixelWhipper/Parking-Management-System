  <property name="styleSheet">
   <string notr="true">
    QMainWindow {
        background-color: #F5F7FA; /* Light grey background */
    }
    /* Header Styling */
    #headerFrame {
        background-color: #FFFFFF;
        border-bottom: 1px solid #E0E0E0;
        padding: 10px 20px;
    }
    #adminLabel {
        font-family: &quot;Inter&quot;;
        font-size: 24px;
        font-weight: bold;
        color: #333333;
    }
    #adminLabel .highlight {
        color: #4285F4; /* Blue accent for 'Admin' */
    }
    #menuIconLabel, #userIconLabel {
        font-size: 28px;
        color: #666666;
    }

    /* Sidebar Styling */
    #sidebarFrame {
        background-color: #FFFFFF;
        border-right: 1px solid #E0E0E0;
    }
    #sidebarListWidget {
        border: none;
        background-color: transparent;
        padding: 10px 0;
    }
    #sidebarListWidget::item {
        padding: 12px 20px;
        color: #666666;
        font-family: &quot;Inter&quot;;
        font-size: 15px;
        border-left: 4px solid transparent; /* For active indicator */
    }
    #sidebarListWidget::item:selected {
        background-color: #E6F0FF; /* Light blue background for selected */
        color: #4285F4; /* Blue text for selected */
        border-left: 4px solid #4285F4; /* Blue indicator for selected */
        font-weight: bold;
    }
    #sidebarListWidget::item:hover:!selected {
        background-color: #F0F2F5; /* Slightly darker hover for unselected */
    }
    /* Style for icons in list items (if using iconMode) */
    #sidebarListWidget QAbstractItemView::indicator {
        width: 24px; /* Adjust as needed */
        height: 24px;
        margin-right: 10px;
    }

    /* Dashboard Cards Styling */
    /* Target QFrames whose objectName starts with 'card' */
    QFrame[objectName^=&quot;card&quot;] {
        background-color: #FFFFFF;
        border-radius: 8px;
        box-shadow: 0px 2px 5px rgba(0, 0, 0, 0.05); /* Subtle shadow */
    }
    /* Target QLabels with the custom dynamic property 'cssClass' set to 'cardValue' */
    QLabel[cssClass=&quot;cardValue&quot;] {
        font-family: &quot;Inter&quot;;
        font-size: 36px;
        font-weight: bold;
        color: #333333;
    }
    /* Target QLabels with the custom dynamic property 'cssClass' set to 'cardTitle' */
    QLabel[cssClass=&quot;cardTitle&quot;] {
        font-family: &quot;Inter&quot;;
        font-size: 14px;
        color: #666666;
    }
    /* Target QLabels with the custom dynamic property 'cssClass' set to 'cardIcon' */
    QLabel[cssClass=&quot;cardIcon&quot;] {
        font-size: 40px; /* Adjust icon size */
        padding: 8px;
        border-radius: 50%;
    }
    #carIcon1 { background-color: #F8FBF8; color: #6FCF97; } /* Green */
    #carIcon2 { background-color: #F8F0FF; color: #BB6BD9; } /* Purple */
    #carIcon3 { background-color: #E6F0FF; color: #4285F4; } /* Blue */
    #carIcon4 { background-color: #FFF0E6; color: #FF9F42; } /* Orange */
    #userIcon { background-color: #E6F0FF; color: #4285F4; }
    #categoryIcon { background-color: #F8FBF8; color: #6FCF97; }

    /* Footer / Main Content Text */
    #footerTextLabel {
        font-family: &quot;Inter&quot;;
        font-size: 16px;
        color: #999999;
        text-align: center;
        padding: 20px;
    }
   </string>
